# PostgreSQL/Supabase 数据库配置
# 使用 docker-compose.supabase.yml 中配置的本地 Supabase 实例
DATABASE_HOST=localhost
DATABASE_PORT=55432
DATABASE_USER=postgres
DATABASE_PASSWORD=your-super-secret-password
DATABASE_NAME=postgres

# JWT 密钥（生产环境请修改）
JWT_SECRET=travel-jwt-secret-change-in-production
JWT_ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=60
REFRESH_TOKEN_EXPIRE_DAYS=7

# AI Providers (AgentScope)
OPENAI_API_KEY=your-openai-api-key
OPENAI_BASE_URL=https://api.openai.com/v1
OPENAI_MODEL=gpt-4
ANTHROPIC_API_KEY=your-anthropic-api-key
ANTHROPIC_BASE_URL=https://api.anthropic.com
ANTHROPIC_MODEL=claude-3-sonnet-20240229
TONGYI_API_KEY=your-tongyi-api-key
TONGYI_BASE_URL=https://dashscope.aliyuncs.com/compatible-mode/v1
TONGYI_MODEL=qwen-max

# App Settings
APP_NAME=Travel Planner
APP_VERSION=1.0.0
DEBUG=True
